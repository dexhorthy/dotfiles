
DIR=ex
SYM_LINK=Fx
SOCKET=Fx
PIPE=dx
EXE=gx
BLOCK_SP=Dx
CHAR_SP=Dx
EXE_SUID=hb
EXE_GUID=ad
DIR_STICKY=Ex
